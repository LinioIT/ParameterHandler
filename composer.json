{
    "name": "linio/composer-parameter-handler",
    "description": "Composer script handling your ignored parameter file",
    "keywords": ["parameters management"],
    "homepage": "https://github.com/LinioIT/ParameterHandler",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "require": {
        "php": "~5.4",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpunit/phpunit": "~4.7",
        "symfony/filesystem": "~2.2"
    },
    "autoload": {
        "psr-4": { "Incenteev\\ParameterHandler\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
